pipeline TestPipeline {

  block TestExtractor oftype TestSheetExtractor {
  }

  valuetype TestValueType {
    property Index oftype integer;
    property Name oftype text;
    property Flag oftype boolean;
  }

  transform Parser {
    from r oftype Collection<text>;
    to t oftype TestValueType;

    t: { Index: asInteger (r . "Index"), Name: r . "Name", Flag: asBoolean (r . "Flag") };
  }

  block TestBlock oftype TableInterpreter {
    header: true;
    columns: TestValueType;
    parseWith: Parser;
  }

  block TestLoader oftype TestTableLoader {
  }

  TestExtractor -> TestBlock -> TestLoader;
}
