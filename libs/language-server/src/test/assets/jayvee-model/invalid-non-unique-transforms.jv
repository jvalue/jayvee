transform DuplicateTransformName {
  from inputParam oftype decimal;
  to result oftype integer;

  result: floor(inputParam);
}

transform DuplicateTransformName {
  from inputParam oftype decimal;
  to inputParam oftype integer;

  result: ceil(inputParam);
}
