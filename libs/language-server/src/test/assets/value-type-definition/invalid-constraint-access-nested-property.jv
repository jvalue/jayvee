valuetype NaturalNumbersWithZero {
  property number oftype integer;

  constraint IsPositive: number >= 0;
}

valuetype NaturalNumbersWithoutZero {
  property naturalNumber oftype NaturalNumbersWithZero ;

  constraint NotZero: naturalNumber.nonExistent != 0;
}
