use * from "./CountryCodeAlpha2.jv";
use * from "./CountryCodeAlpha3.jv";
use * from "./CountryCodeNumeric.jv";

/**
* Any code (alppha2, alpha3, numeric) for countries as defined by ISO 3166.
*/
publish valuetype CountryCode oftype text {
  constraints: [
    CountryCodeAlpha2Constraint,
    CountryCodeAlpha3Constraint,
    CountryCodeNumericConstraint
  ];
}